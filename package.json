  "bugs": {
    "url": "https://github.com/XiaochanXia/octopus-cli/issues"
  },
  "homepage": "https://github.com/XiaochanXia/octopus-cli#readme",
  "dependencies": {
    "colors": "^1.3.3"
  },
  "devDependencies": {
    "eslint-config-octopus": "1",
    "commitlint-config-octopus": "1"
  },
  "commitlint": {
